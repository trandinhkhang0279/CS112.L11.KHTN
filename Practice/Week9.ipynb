   "source": [
    "## 5. Programming\n",
    "```python\n",
    "n = int(input().strip())\n",
    "a = list(map(int, input().strip().split()))\n",
    "\n",
    "H = [0] * (n+1)\n",
    "a = [0] + a\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    H[i] = H[i-1] + a[i]*(a[i]+11)*(a[i]+17)\n",
    "\n",
    "def get_H(l,r):\n",
    "    return H[r]-H[l-1]\n",
    "\n",
    "ans = []\n",
    "\n",
    "def solve(x, ans):\n",
    "    n1 = 0\n",
    "    n2 = 0\n",
    "    s1 = -1\n",
    "    s2 = -1\n",
    "    for i in range(1, n+1, x):\n",
    "        s = get_H(i,i+x-1)\n",
    "        if s==s1 or s1==-1:\n",
    "            n1+=1\n",
    "            s1=s\n",
    "        elif s==s2 or s2==-1:\n",
    "            n2+=1\n",
    "            s2=s\n",
    "        else: return\n",
    "    if n1>0 and n2>0:\n",
    "        ans += [(x,n1,n2)]\n",
    "\n",
    "for i in range(1,n):\n",
    "    if n%i==0:\n",
    "        solve(i, ans)\n",
    "\n",
    "if len(ans)==0:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(len(ans))\n",
    "    for x in ans:\n",
    "        print(*x)\n",
    "```"
